doctype html
html 
    head 
        title=`Home Page`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body

        h1 Custom Object Table
        a(href='/update-cobj') Add/Update this Table

        p(style='color: red') #{error}

        table(border)
            thead 
                tr
                    th Name 
                    th Author 
                    th Publisher 
            tbody 
                each book in books
                    tr 
                        td #{book.properties.book_name}
                        td #{book.properties.name}
                        td #{book.properties.publisher}